    <div>
      <form action="/discover/search">
      <input type="text" name="instrument" placeholder="Instruments">
      <button type="submit">Search</button>
      </form>
    </div>


<div class="row">
{{#each musiciansFrDB}} 
<div class="col">
  <div class="card h-100">
    <img
      class="card-img-top"
      src="{{this.imageUrl}}"
      alt=""
    />
    <div class="card-body">
      <h5 class="card-title">{{this.username}}</h5>
      <p class="card-text"> {{this.instruments}}
    </div>
    <div class="card-footer">
      <small class="text-muted">
          <a href="{{this._id}}">See more</a></p>
      </small>
    </div>
  </div>
</div>
 {{/each}}
</div>

